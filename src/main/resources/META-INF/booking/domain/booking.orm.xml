<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm" version="3.2">
  <package>com.github.booking.domain.booking</package>
  <access>FIELD</access>
  <sequence-generator name="Booking" sequence-name="booking_s"/>
  <entity class="Booking">
    <table name="booking"/>
    <attributes>
      <id name="id">
        <column name="id_" updatable="false"/>
        <generated-value strategy="SEQUENCE" generator="Booking"/>
      </id>
      <basic name="status">
        <column name="status"/>
        <enumerated>STRING</enumerated>
      </basic>
      <embedded name="showId">
        <attribute-override name="value">
          <column name="show_id" updatable="false"/>
        </attribute-override>
      </embedded>
      <embedded name="bookingId">
        <attribute-override name="value">
          <column name="booking_id" updatable="false"/>
        </attribute-override>
      </embedded>
    </attributes>
  </entity>
  <embeddable class="BookingId">
    <attributes>
      <basic name="value"/>
    </attributes>
  </embeddable>
</entity-mappings>
